<suite name="ProfilePage" verbose="1">
    <test name="ProfilePagePosts" preserve-order="true">
        <classes>
            <class name="tests.ValidLogin">
                <methods>
                    <include name="correctLogin"/>
                </methods>
            </class>
            <class name="tests.UserPosts">
                <methods>
                    <include name="numberOfPosts" dependsOnMethods="correctLogin"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
